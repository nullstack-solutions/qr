    "next": "14.1.0",
    "papaparse": "5.4.1",
    "qr-code-styling": "1.6.0",
    "qrcode": "1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "20.11.19",
    "@types/pngjs": "^6.0.4",
    "@types/papaparse": "^5.3.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.18",
    "eslint": "8.56.0",
