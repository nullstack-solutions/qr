{
  "name": "qr-suite",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "serve out -l 3000",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck:e2e": "tsc -p tests/e2e/tsconfig.json",
    "pretest:e2e": "node scripts/pretest-e2e.mjs",
    "test": "npm run typecheck && node --test tests/*.test.mjs",
    "test:e2e": "node scripts/run-e2e.mjs",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@twa-dev/sdk": "^8.0.2",
    "@zxing/browser": "0.1.5",
    "classnames": "2.5.1",
    "idb": "8.0.3",
    "jszip": "3.10.1",
    "next": "16.0.1",
    "papaparse": "5.5.3",
    "awesome-qr": "2.1.5-rc.0",
    "qrcode": "1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "24.9.2",
    "@types/pngjs": "^6.0.5",
    "@types/papaparse": "^5.3.16",
    "@types/qrcode": "^1.5.6",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.39.0",
    "eslint-config-next": "16.0.1",
    "jsqr": "^1.4.0",
    "pngjs": "^7.0.0",
    "serve": "^14.2.5",
    "typescript": "5.9.3"
  }
}
